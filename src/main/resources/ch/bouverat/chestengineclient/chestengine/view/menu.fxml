<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="TOP_CENTER" prefHeight="317.0" prefWidth="556.0" spacing="20.0" style="-fx-background-color: grey;" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="ch.bouverat.chestengineclient.chestengine.controller.MenuController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Text fill="WHITE" stroke="WHITE" text="Welcome to ChestEngine" textAlignment="CENTER">
         <font>
            <Font name="System Bold Italic" size="39.0" />
         </font>
         <VBox.margin>
            <Insets bottom="40.0" />
         </VBox.margin>
      </Text>
      <Button contentDisplay="TOP" mnemonicParsing="false" onAction="#joinGame" prefHeight="68.0" prefWidth="310.0" text="Join Game" textFill="#786e6e" textOverrun="CLIP">
         <font>
            <Font size="36.0" />
         </font>
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin>
      </Button>
      <Button contentDisplay="TOP" mnemonicParsing="false" onAction="#createGame" prefHeight="68.0" prefWidth="310.0" text="Create game" textFill="#786e6e" textOverrun="CLIP">
         <font>
            <Font size="36.0" />
         </font>
         <VBox.margin>
            <Insets bottom="5.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
